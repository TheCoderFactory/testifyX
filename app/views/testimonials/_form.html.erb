<%= simple_form_for(@testimonial) do |f| %>
  <%= f.error_notification %>

  
  	<div class="form-group">
    	<%= f.input :star_rating, input_html: { class: 'form-control'} %>
    </div>
  	<div class="form-group">
    	<%= f.input :review, input_html: { class: 'form-control'} %>
    </div>
  	<div class="form-group">
    	<%= f.association :business, input_html: { class: 'form-control'} %>
    </div>
  	<div class="form-group">
    	<%= f.association :user, input_html: { class: 'form-control'} %>
    </div>
  

  <div class="form-actions">
    <%= f.submit 'Submit', class: 'btn btn-primary' %>
  </div>
<% end %>
